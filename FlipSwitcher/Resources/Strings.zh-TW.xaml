<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Chinese Traditional -->
    
    <!-- Main Window -->
    <system:String x:Key="AppTitle">FlipSwitcher</system:String>
    <system:String x:Key="WindowsCount">個視窗</system:String>
    <system:String x:Key="SearchPlaceholder">搜尋視窗...</system:String>
    <system:String x:Key="NoWindowsFound">未找到視窗</system:String>
    <system:String x:Key="TryDifferentSearch">嘗試其他搜尋詞</system:String>
    <system:String x:Key="Minimized">已最小化</system:String>
    <system:String x:Key="ElevatedWindow">系統管理員</system:String>
    <system:String x:Key="MonitorPrefix">顯示器 </system:String>
    
    <!-- Footer Hints -->
    <system:String x:Key="HintEnter">Enter</system:String>
    <system:String x:Key="HintSwitch">切換</system:String>
    <system:String x:Key="HintNav">導航</system:String>
    <system:String x:Key="HintSearch">搜尋</system:String>
    <system:String x:Key="HintClose">關閉</system:String>
    <system:String x:Key="HintSettings">設定</system:String>
    <system:String x:Key="HintExit">結束</system:String>
    
    <!-- Settings Window -->
    <system:String x:Key="Settings">設定</system:String>
    <system:String x:Key="SettingsClose">關閉</system:String>
    
    <!-- Settings - Language -->
    <system:String x:Key="SettingsLanguage">語言</system:String>
    <system:String x:Key="SettingsLanguageDesc">選擇您偏好的語言</system:String>
    <system:String x:Key="LanguageEnglish">English</system:String>
    <system:String x:Key="LanguageChinese">简体中文</system:String>
    <system:String x:Key="LanguageChineseTraditional">繁體中文</system:String>
    
    <!-- Settings - Hotkey -->
    <system:String x:Key="SettingsHotkey">快速鍵</system:String>
    <system:String x:Key="SettingsAltSpace">Alt + Space</system:String>
    <system:String x:Key="SettingsAltSpaceDesc">使用 Alt + Space 啟動 FlipSwitcher</system:String>
    <system:String x:Key="SettingsAltTab">Alt + Tab</system:String>
    <system:String x:Key="SettingsAltTabDesc">使用 FlipSwitcher 取代系統 Alt+Tab</system:String>
    <system:String x:Key="SettingsReplacesSystem">取代系統</system:String>
    <system:String x:Key="SettingsActiveHotkey">目前快速鍵:</system:String>
    
    <!-- Settings - Appearance -->
    <system:String x:Key="SettingsAppearance">外觀</system:String>
    <system:String x:Key="SettingsTheme">主題</system:String>
    <system:String x:Key="SettingsThemeDesc">選擇淺色或深色模式</system:String>
    <system:String x:Key="ThemeDark">深色</system:String>
    <system:String x:Key="ThemeLight">淺色</system:String>
    <system:String x:Key="ThemeLatte">拿鐵</system:String>
    <system:String x:Key="ThemeMocha">摩卡</system:String>
    <system:String x:Key="ThemeSystem">跟隨系統</system:String>
    
    <!-- Settings - Behavior -->
    <system:String x:Key="SettingsBehavior">行為</system:String>
    <system:String x:Key="SettingsRunAsAdmin">以系統管理員身分執行</system:String>
    <system:String x:Key="SettingsRunAsAdminDesc">切換到提升權限視窗時需要 (需重新啟動)</system:String>
    <system:String x:Key="SettingsAdminEnabled">已啟用</system:String>
    <system:String x:Key="SettingsAdminDisabled">未啟用</system:String>
    <system:String x:Key="SettingsStartWithWindows">開機自動啟動</system:String>
    <system:String x:Key="SettingsStartWithWindowsDesc">登入時自動啟動 FlipSwitcher</system:String>
    <system:String x:Key="SettingsHideOnFocusLost">失去焦點時隱藏</system:String>
    <system:String x:Key="SettingsHideOnFocusLostDesc">點擊外部區域時自動隱藏</system:String>
    <system:String x:Key="SettingsPinyinSearch">拼音搜尋</system:String>
    <system:String x:Key="SettingsPinyinSearchDesc">使用拼音首字母或全拼搜尋視窗</system:String>
    <system:String x:Key="SettingsShowMonitorInfo">顯示顯示器資訊</system:String>
    <system:String x:Key="SettingsShowMonitorInfoDesc">顯示每個視窗所在的顯示器編號</system:String>
    
    <!-- Settings - Updates -->
    <system:String x:Key="SettingsUpdates">更新</system:String>
    <system:String x:Key="SettingsCheckForUpdatesAuto">自動檢查更新</system:String>
    <system:String x:Key="SettingsCheckForUpdatesAutoDesc">應用程式啟動時自動檢查更新</system:String>
    <system:String x:Key="SettingsCheckForUpdates">檢查更新</system:String>
    <system:String x:Key="SettingsCheckingUpdates">檢查中...</system:String>
    
    <!-- Settings - About -->
    <system:String x:Key="SettingsAbout">關於</system:String>
    <system:String x:Key="SettingsAboutDesc">採用 Fluent 2 設計的現代 Alt-Tab 替代品</system:String>
    <system:String x:Key="SettingsViewOnGitHub">在 GitHub 上查看</system:String>
    
    <!-- Tray Menu -->
    <system:String x:Key="TrayShow">顯示</system:String>
    <system:String x:Key="TraySettings">設定</system:String>
    <system:String x:Key="TrayRestart">重新啟動</system:String>
    <system:String x:Key="TrayExit">結束</system:String>
    
    <!-- MessageBox -->
    <system:String x:Key="MsgRestartRequired">FlipSwitcher 需要以系統管理員權限重新啟動。是否立即重新啟動？</system:String>
    <system:String x:Key="MsgRestartRequiredNormal">FlipSwitcher 需要以一般使用者權限重新啟動。是否立即重新啟動？</system:String>
    <system:String x:Key="MsgRestartFailed">無法重新啟動應用程式。請手動重新啟動。</system:String>
    <system:String x:Key="MsgRestartFailedTitle">重新啟動失敗</system:String>
    <system:String x:Key="MsgRestartRequiredTitle">需要重新啟動</system:String>
    <system:String x:Key="MsgStartupFailed">無法啟用開機自動啟動。請嘗試以系統管理員身分執行。</system:String>
    <system:String x:Key="MsgStartupDisabledFailed">無法停用開機自動啟動。</system:String>
    <system:String x:Key="MsgAtLeastOneHotkey">至少需要啟用一個快速鍵。</system:String>
    <system:String x:Key="MsgCannotCloseElevatedWindow">無法關閉系統管理員視窗，請在設定中啟用「以系統管理員身分執行」。</system:String>
    <system:String x:Key="MsgCannotSwitchToElevatedWindow">無法切換到系統管理員視窗，請在設定中啟用「以系統管理員身分執行」。</system:String>
    <system:String x:Key="MsgErrorOccurred">發生錯誤：{0}</system:String>
    <system:String x:Key="MsgErrorTitle">FlipSwitcher 錯誤</system:String>
    <system:String x:Key="MsgUpdateAvailable">發現 FlipSwitcher 新版本 {0}！是否立即下載？</system:String>
    <system:String x:Key="MsgUpdateAvailableTitle">發現更新</system:String>
    <system:String x:Key="MsgNoUpdateAvailable">您正在使用最新版本。</system:String>
    <system:String x:Key="MsgNoUpdateAvailableTitle">無更新</system:String>
    <system:String x:Key="MsgUpdateCheckFailed">檢查更新失敗。請檢查您的網路連線。</system:String>
    <system:String x:Key="MsgUpdateCheckFailedTitle">更新檢查失敗</system:String>
    
    <!-- Additional Strings -->
    <system:String x:Key="SettingsWindowTitle">FlipSwitcher 設定</system:String>
    <system:String x:Key="VersionPreview">預覽版</system:String>
    <system:String x:Key="VersionFormat">FlipSwitcher v{0}</system:String>
    <system:String x:Key="DefaultFontDisplay">預設 (Segoe UI Variable)</system:String>
    <system:String x:Key="HotkeyNone">無</system:String>
    <system:String x:Key="HotkeySeparator"> / </system:String>
    <system:String x:Key="SettingsFont">字體</system:String>
    <system:String x:Key="SettingsFontDesc">選擇應用程式使用的字體</system:String>
    <system:String x:Key="TrayToolTip">FlipSwitcher</system:String>

    <!-- Dialog Buttons -->
    <system:String x:Key="DialogOK">確定</system:String>
    <system:String x:Key="DialogCancel">取消</system:String>
    <system:String x:Key="DialogYes">是</system:String>
    <system:String x:Key="DialogNo">否</system:String>

</ResourceDictionary>
